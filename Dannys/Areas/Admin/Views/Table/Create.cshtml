@model TableCreateDto
<form method="post" class="category" style="width: 100%">
    <div class="content-wrapper" style=" height:100vh">
        <div class="page-header">
            <h3 class="page-title"> Table Create </h3>

        </div>
        <div class="row">
            <div class="col-md-6 grid-margin stretch-card" style=" width:100%">
                <div class="card">
                    <div class="card-body">
                        <form method="post" class="forms-sample">
                            <div asp-validation-summary="ModelOnly" class="text-danger">  </div>
                            <div class="form-group">
                           
                            <div class="form-group">
                                <label asp-for="PersonCount" for="exampleInputUsername1"></label>
                                <input  class="form-control" id="exampleInputName" asp-for="PersonCount" placeholder="PersonCount">
                                <span asp-validation-for="PersonCount" class="text-danger"></span>
                            </div>
                            <button type="submit" class="btn btn-primary me-2">Submit</button>
                            <button class="btn btn-dark">Cancel</button>
                        </form>
                    </div>
                </div>
            </div>

        </div>
    </div>
</form>

