@model SliderUpdateDto
<form method="post" class="category" style="width: 100%">
    <div class="content-wrapper" style=" height:100vh">
        <div class="page-header">
            <h3 class="page-title"> Slider Update </h3>
           
        </div>
        <div class="row">
            <div class="col-md-6 grid-margin stretch-card" style=" width:100%">
                <div class="card">
                    <div class="card-body">
                        <form class="forms-sample">
                            <div class="form-group">
                                <label asp-for="Name" for="exampleInputUsername1">Name</label>
                                <input type="text" class="form-control" id="exampleInputName" asp-for="Name" placeholder="Name">
                                <span asp-validation-for="Name" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="File" for="exampleInputUsername1"></label>
                                <input class="form-control" id="exampleInputName" asp-for="File" placeholder="Enter Image">
                                <span asp-validation-for="File" class="text-danger"></span>
                            </div>
                            <div asp-validation-summary="ModelOnly" class="text-danger">  </div>
                            <button type="submit" class="btn btn-primary me-2">Submit</button>
                            <button class="btn btn-dark">Cancel</button>
                        </form>
                    </div>
                </div>
            </div>

        </div>
    </div>
</form>

